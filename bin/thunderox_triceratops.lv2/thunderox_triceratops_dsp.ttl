@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix opts: <http://lv2plug.in/ns/ext/options#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://thunderox.com/thunderOx_triceratops>
    a lv2:InstrumentPlugin, lv2:Plugin ;

    lv2:extensionData opts:interface ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature opts:options ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    opts:supportedOption <http://lv2plug.in/ns/ext/buf-size#nominalBlockLength> ,
                         <http://lv2plug.in/ns/ext/buf-size#maxBlockLength> ,
                         <http://lv2plug.in/ns/ext/parameters#sampleRate> ;

    ui:ui <http://thunderox.com/thunderOx_triceratops#UI> ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, atom:AtomPort ;
        lv2:index 2 ;
        lv2:name "Events Input" ;
        lv2:symbol "lv2_events_in" ;
        rsz:minimumSize 2048 ;
        atom:bufferType atom:Sequence ;
        atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ;
        atom:supports <http://lv2plug.in/ns/ext/time#Position> ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name """""" ;
        lv2:symbol "lv2_port_2" ;
        lv2:default 512.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name """triceratops Time""" ;
        lv2:symbol "triceratops_time" ;
        lv2:default 0.500000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name """triceratops Amount""" ;
        lv2:symbol "triceratops_amount" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name """triceratops Volume""" ;
        lv2:symbol "triceratops_volume" ;
        lv2:default 0.800000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name """""" ;
        lv2:symbol "lv2_port_6" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name """""" ;
        lv2:symbol "lv2_port_7" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name """""" ;
        lv2:symbol "lv2_port_8" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#expensive> ,
                         <http://kxstudio.sf.net/ns/lv2ext/props#NonAutomable> ;
    ] ;

    doap:name """thunderOx_triceratops""" ;
    doap:license """ISC""" ;

    doap:maintainer [
        foaf:name """Nick S. Bailey""" ;
    ] ;

    lv2:microVersion 0 ;
    lv2:minorVersion 2 .

